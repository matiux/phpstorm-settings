<application>
  <component name="CodeVisionSettings">
    <option name="codeVisionGroupToPosition">
      <map>
        <entry key="inheritors" value="Right" />
        <entry key="references" value="Right" />
        <entry key="vcs.code.vision" value="Default" />
      </map>
    </option>
    <option name="disabledCodeVisionProviderIds">
      <set>
        <option value="vcs.code.vision" />
      </set>
    </option>
    <option name="visibleMetricsAboveDeclarationCount" value="6" />
    <option name="visibleMetricsNextToDeclarationCount" value="6" />
  </component>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="AUTOREPARSE_DELAY" value="0" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="PHP" value="true" />
      </map>
    </option>
    <option name="SHOW_BREADCRUMBS" value="false" />
    <option name="ENABLE_RENDERED_DOC" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="disabledHintProviderIds">
      <set>
        <option value="JavaScript.js.chain.hints" />
        <option value="JavaScript.js.type.hints" />
        <option value="TypeScript.ts.chain.hints" />
        <option value="TypeScript.ts.enum.hints" />
        <option value="TypeScript.ts.type.hints" />
      </set>
    </option>
    <option name="lastViewedProviderKeyId" value="SQL.parameter.hints.old.sql.show.column.names.for.asterisk" />
    <settingsMapElement>
      <JavaScript.js.type.hints>
        <Settings>
          <option name="showForInferredParameters" value="false" />
        </Settings>
      </JavaScript.js.type.hints>
      <TypeScript.ts.type.hints>
        <Settings>
          <option name="showForInferredParameters" value="false" />
        </Settings>
      </TypeScript.ts.type.hints>
      <SQL.sql.join.cardinality.hints>
        <Settings>
          <option name="showForFullJoin" value="true" />
          <option name="showForInnerJoin" value="true" />
          <option name="showForLeftJoin" value="true" />
          <option name="showForRightJoin" value="true" />
        </Settings>
      </SQL.sql.join.cardinality.hints>
    </settingsMapElement>
  </component>
  <component name="PhpEditorOptions">
    <option name="SHOW_CLASS_SEPARATORS" value="true" />
    <option name="ALWAYS_ENABLE_PHP_BACKGROUND_HIGHLIGHTING" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>